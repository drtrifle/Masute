{
    "SongLyrics": [
        {
            "JpnStr": "靴紐[くつひも]が解[ほど]けてる 木漏[こも]れ日[び]は足[あし]を舐[な]む",
            "EngStr": "My shoelaces are undone, the sunbeams filter through the trees"
        },
        {
            "JpnStr": "息[いき]を吸[す]う音[おと]だけ聞[き]こえてる",
            "EngStr": "The sound of my breath is the only thing that I hear"
        },
        {
            "JpnStr": "貴方[あなた]は今[いま]立[た]ち上[あ]がる 古[ふる]びた椅子[いす]の上[うえ]から",
            "EngStr": "You stand up from that creaky old chair"
        },
        {
            "JpnStr": "柔[やわ]らかい麻[あさ]の匂[にお]いがする",
            "EngStr": "The gentle smell of hemp tickles my nose"
        },
        {
            "JpnStr": "",
            "EngStr": ""
        },
        {
            "JpnStr": "遥[はる]か遠[とお]くへ まだ遠[とお]くへ",
            "EngStr": "Into the horizon, and the great beyond"
        },
        {
            "JpnStr": "僕[ぼく]らは身体[からだ]も脱[ぬ]ぎ去[さ]って",
            "EngStr": "We take off and leave our bodies behind"
        },
        {
            "JpnStr": "まだ遠[とお]くへ 雲[くも]も越[こ]えてまだ向[む]こうへ",
            "EngStr": "Even further, to a place higher than the clouds"
        },
        {
            "JpnStr": "風[かぜ]に乗[の]って",
            "EngStr": "We ride the wind to places that defy even our imagination"
        },
        {
            "JpnStr": "僕[ぼく]の想像力[そうぞうりょく]という重力[じゅうりょく]の向[む]こうへ",
            "EngStr": "Flying ever"
        },
        {
            "JpnStr": "まだ遠[とお]くへ まだ遠[とお]くへ",
            "EngStr": "Further away"
        },
        {
            "JpnStr": "海[うみ]の方[ほう]へ",
            "EngStr": "Ever towards the sea"
        },
        {
            "JpnStr": "",
            "EngStr": ""
        },
        {
            "JpnStr": "靴紐[くつひも]が解[ほど]けてる 蛇[へび]みたいに跳[は]ね遊[あそ]ぶ",
            "EngStr": "My shoelaces are undone, like snakes they bound around and play"
        },
        {
            "JpnStr": "貴方[あなた]の靴[くつ]が気[き]になる",
            "EngStr": "I wonder if your shoes are undone too"
        },
        {
            "JpnStr": "僕[ぼく]らは今[いま]歩[ある]き出[だす] 潮風[しおかぜ]は肌[はだ]を舐[な]む",
            "EngStr": "The sea breeze licks our skin, as we walk along"
        },
        {
            "JpnStr": "手[て]を引[ひ]かれるままの道[みち]",
            "EngStr": "The path your hands lead me down"
        },
        {
            "JpnStr": "",
            "EngStr": ""
        },
        {
            "JpnStr": "さぁまだ遠[とお]くへ まだ遠[とお]くへ",
            "EngStr": "Into the horizon, and the great beyond"
        },
        {
            "JpnStr": "僕[ぼく]らはただの風[かぜ]になって",
            "EngStr": "We become the wind and fly"
        },
        {
            "JpnStr": "まだ遠[とお]くへ 雲[くも]も越[こ]えてまだ向[む]こうへ",
            "EngStr": "Even further, to a place higher than the clouds"
        },
        {
