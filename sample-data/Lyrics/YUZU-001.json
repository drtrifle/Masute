{
    "SongLyrics": [
        {
            "JpnStr": "海沿[うみぞ]いの道[みち]を 手[て]をつないで歩[ある]いた",
            "EngStr": "We walked hand in hand along the road by the sea"
        },
        {
            "JpnStr": "あの日[ひ]の約束[やくそく]はもう叶[かな]わない夜空[よぞら]の星[ほし]",
            "EngStr": "That day's promise will no longer come true, stars in the night sky"
        },
        {
            "JpnStr": "通[とお]り過[す]ぎてゆく 人[ひと]も景色達[けしきたち]も",
            "EngStr": "The people and scenery that pass by"
        },
        {
            "JpnStr": "気[き]づいた時[とき]は いつもおいてけぼりになってた",
            "EngStr": "When I realized I was always left behind"
        },
        {
            "JpnStr": "",
            "EngStr": ""
        },
        {
            "JpnStr": "季節変[きせつが]わり今[いま]も君[きみ]の事[こと] 想[おも]い出[だ]してしまうけれど",
            "EngStr": "The seasons change, and I still think of you"
        },
        {
            "JpnStr": "何[なに]も無[な]かったような顔[かお]して 今日[きょう]も街[まち]に溶[と]けて行[い]く",
            "EngStr": "But today, too, I'm melting into the city with a face as if nothing had happened"
        },
        {
            "JpnStr": "",
            "EngStr": ""
        },
        {
            "JpnStr": "さよなら もう新[あたら]しい明日[あした]へ歩[ある]き出[だ]した",
            "EngStr": "Goodbye, I've already walked to a new tomorrow"
        },
        {
            "JpnStr": "最後[さいご]の強[つよ]がりきっと これが二人[ふたり]の為[ため]だよね?",
            "EngStr": "It's the last time I'll be strong. This is for the both of us, right?"
        },
        {
            "JpnStr": "待[ま]ち合[あ]わせ場所[ばしょ]いつもの桜木町[さくらぎちょう]に君[きみ]は もう来[こ]ない",
            "EngStr": "At the usual meeting place in Sakuragicho, you're no longer coming."
        },
        {
            "JpnStr": "",
            "EngStr": ""
        },
        {
            "JpnStr": "大[おお]きな観覧車[かんらんしゃ]「花火[はなび]みたいだね」 って",
            "EngStr": "The big Ferris wheel 'looks like fireworks', you said."
        },
        {
            "JpnStr": "笑[わら]った君[きみ]の横顔[よこがお] 時間[じかん]が止[と]まって欲[ほ]しかった",
            "EngStr": "I wanted time to stop."
        },
        {
            "JpnStr": "",
            "EngStr": ""
        },
        {
            "JpnStr": "心変[こころが]わり今[いま]は責[せ]めても 違[ちが]う誰[だれ]かの元[もと]へ",
            "EngStr": "Even if I blame you for changing my mind now, I'll go to someone else."
        },
        {
            "JpnStr": "そして いつの日[ひ]か忘[わす]れてゆく 君[きみ]の笑顔[えがお]も泪[なみだ]も",
            "EngStr": "And someday I'll forget your smile and your tears"
        },
        {
            "JpnStr": "",
            "EngStr": ""
        },
        {
            "JpnStr": "ありがとう さあ振[ふ]り返[かえ]らずに行[ゆ]けばいい",
            "EngStr": "Thank you, don't look back, just go"
        },
        {
            "JpnStr": "いつの日[ひ]かまた 笑[わら]って話[はな]せる時[とき]が来[く]るさ",
            "EngStr": "Someday we'll be able to laugh and talk again"
        },
        {
            "JpnStr": "初[はじ]めて君[きみ]と口付[くちづ]けた 桜木町[さくらぎちょう]で最後[さいご]の手[て]を振[ふ]るよ",
            "EngStr": "I'll wave my hand for the last time in Sakuragicho, where I first kissed you"
        },
        {
            "JpnStr": "",
            "EngStr": ""
        },
        {
            "JpnStr": "変[か]わり続[つづ]けてく 見慣[みな]れてた街並[まちなみ]も",
            "EngStr": "The streets that I'm used to seeing keep changing"
        },
        {
            "JpnStr": "だけど今[いま]も目[め]を閉[と]じれば あの日[ひ]の二人[ふたり]がそこにはいる",
            "EngStr": "But even now, if I close my eyes, the two of us from that day are still there"
        },
        {
            "JpnStr": "",
            "EngStr": ""
        },
        {
            "JpnStr": "繋[つな]いだ その手[て]をいつまでも離[はな]したくなかった",
            "EngStr": "I didn't want to let go of the hand we held forever"
        },
        {
            "JpnStr": "それでも行[い]かなくちゃ 僕[ぼく]らが見[み]つけた答[こた]えだから",
            "EngStr": "I still have to go, because that's the answer we've found"
        },
        {
            "JpnStr": "",
            "EngStr": ""
        },
        {
            "JpnStr": "さよなら もう新[あたら]しい明日[あした]へ歩[ある]き出[だ]した",
            "EngStr": "Goodbye, we've already started walking to a new tomorrow"
        },
        {
            "JpnStr": "最後[さいご]の 強[つよ]がり きっと これが二人[ふたり]の為[ため]だよね?",
            "EngStr": "I'm sure this is for the both of us, right?"
        },
        {
            "JpnStr": "待[ま]ち合[あ]わせ場所[ばしょ]いつもの桜木町[さくらぎちょう]に君[きみ]は もう来[こ]ない",
            "EngStr": "At the usual meeting place in Sakuragicho, you're no longer there."
        },
        {
            "JpnStr": "初[はじ]めて君[きみ]と口付[くちづ]けた桜木町[さくらぎちょう]で最後[さいご]の手[て]を振[ふ]るよ",
            "EngStr": "I'll wave my hand for the last time at Sakuragicho, where I first kissed you."
        }
    ],
